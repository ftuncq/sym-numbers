<div class="accordion" id="accordionFaq">
	{% for faq in faqs %}
		<div class="accordion-item wjne7j">
			<h2 class="accordion-header">
				<button class="accordion-button d3ogo9 collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapse-{{ faq.id }}" aria-expanded="false" aria-controls="collapse-{{ faq.id }}">
					{{ faq.name }}
				</button>
			</h2>
			<div id="collapse-{{ faq.id }}" class="accordion-collapse collapse" data-bs-parent="#accordionFaq">
				<div class="accordion-body">
					{{ faq.content|raw }}
				</div>
			</div>
		</div>
	{% endfor %}
</div>